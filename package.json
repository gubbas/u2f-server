{
  "name": "u2f-server",
  "version": "1.0.0",
  "description": "U2F Server Microservice",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "start-js": "node bin/src/app.js",
    "build": "gulp typescript",
    "lint": "tslint -c tslint.json -e \"node_modules/**/*.ts\" -e \"typings/**/*.ts\" \"**/*.ts\"",
    "test": "node_modules/.bin/mocha --exit --compilers ts:ts-node/register 'src/**/*.spec.ts'",
    "test:coverage": "node_modules/.bin/nyc node_modules/.bin/mocha --exit --compilers ts:ts-node/register --require source-map-support/register --full-trace --bail 'src/**/*.spec.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/gulp-nodemon": "0.0.32",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/knex": "^0.14.19",
    "@types/redis": "^2.8.6",
    "@types/restify": "^7.2.3",
    "@types/restify-cors-middleware": "^1.0.1",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "dotenv": "^6.0.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "knex": "^0.15.2",
    "mocha": "^5.2.0",
    "node-couchdb": "^1.3.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "otplib": "^10.0.1",
    "pg": "^7.4.3",
    "qrcode": "^1.2.0",
    "redis": "^2.8.0",
    "restify": "^7.2.1",
    "restify-cors-middleware": "^1.1.1",
    "restify-router": "^0.5.0",
    "sinon": "^6.1.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.1",
    "u2f": "^0.1.3",
    "uuid": "^3.3.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
